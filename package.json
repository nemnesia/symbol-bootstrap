{
  "name": "@nemneshia/symbol-bootstrap",
  "description": "Symbol tool that allows you creating, configuring and running Symbol's networks",
  "version": "2.0.7",
  "author": "Nemneshia <@nemneshia>",
  "type": "module",
  "bin": {
    "sb": "bin/run.js",
    "symbol-bootstrap": "bin/run.js"
  },
  "bugs": "https://github.com/nemneshia/symbol-bootstrap/issues",
  "dependencies": {
    "@inquirer/prompts": "^8.0.1",
    "@noble/ed25519": "^3.0.0",
    "@noble/hashes": "^2.0.1",
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-autocomplete": "^3.2.39",
    "@oclif/plugin-help": "^6.2.36",
    "archiver": "^7.0.1",
    "cross-fetch": "^4.1.0",
    "figlet": "^1.9.4",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.21",
    "memorystream": "^0.3.1",
    "node-stream-zip": "^1.15.0",
    "rxjs": "^7.8.2",
    "semver": "^7.7.3",
    "shx": "^0.4.0",
    "symbol-sdk": "^2.0.7",
    "symbol-statistics-service-typescript-fetch-client": "^1.1.5",
    "tslib": "^2.8.1",
    "utf8": "^3.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.15",
    "@types/archiver": "^7.0.0",
    "@types/chai": "^5.2.3",
    "@types/figlet": "^1.7.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.21",
    "@types/memorystream": "^0.3.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "@types/semver": "^7.7.1",
    "@types/sinon": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "chai": "^6.2.1",
    "coveralls": "^3.1.1",
    "create-ts-index": "^1.14.0",
    "dir-compare": "^5.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globby": "^16.0.0",
    "marked": "^17.0.1",
    "mocha": "^11.7.5",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-stdin": "^1.0.0",
    "nock": "^14.0.10",
    "nyc": "^17.1.0",
    "oclif": "^4.22.50",
    "prettier": "^3.7.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "sinon": "^21.0.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/config",
    "/presets",
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/nemneshia/symbol-bootstrap",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "bin": "symbol-bootstrap",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemneshia/symbol-bootstrap.git"
  },
  "scripts": {
    "doc": "typedoc --out \"ts-docs\" src && touch ./ts-docs/.nojekyll",
    "lint": "eslint --cache src/ test/ --ext .ts",
    "clean": "npx shx rm -rf ./lib tsconfig.build.tsbuildinfo",
    "lint:fix": "eslint src/ test/ --ext .ts --fix",
    "prettier": "prettier --write ./src ./test",
    "style:fix": "npm run create-index-files && npm run prettier && npm run lint:fix && npm run oclif-doc",
    "create-index-files": "cti create ./src -b -n -e commands",
    "postpack": "npx shx rm -f oclif.manifest.json tsconfig.build.tsbuildinfo",
    "posttest": "eslint src/ test/ --ext .ts",
    "oclif-doc": "oclif manifest && oclif readme --multi",
    "oclif-debug": "env DEBUG=* oclif manifest",
    "watch": "tsc -w -p tsconfig.build.json",
    "compile": "tsc -p tsconfig.build.json",
    "build": "npm run clean && npm run compile && oclif manifest",
    "prepack": "npm run build && npm run oclif-doc",
    "test": "nyc --reporter=lcov --extension .ts mocha \"test/**/*.test.ts\"",
    "test-backup": "nyc --reporter=lcov --extension .ts mocha -r ts-node/register --timeout 900000 --forbid-only \"test/**/*.test.ts\"",
    "e2e": "nyc --reporter=lcov --extension .ts mocha -r ts-node/register --timeout 900000 --forbid-only \"test/**/*.e2e.ts\"",
    "coveralls-report": "cat ./coverage/lcov.info | coveralls",
    "version": "echo $npm_package_version",
    "install-cli": "npm pack && npm i -g",
    "clean-network": "symbol-bootstrap clean -t target/bootstrap-test",
    "start-network": "symbol-bootstrap start -t target/bootstrap-test -u 'current'",
    "publish:prd": "npm publish --access public",
    "publish:dry": "npm publish --access public --dry-run"
  },
  "types": "lib/index.d.ts"
}
